<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.getova.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title_activity_home"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.details.DetailsActivity"
            android:label="@string/title_activity_details" />
        <activity android:name=".views.favorites.MyFavoritesListActivity" />
        <activity android:name=".views.order.OrderActivity" />
        <activity
            android:name=".views.menu.MenuTabListActivity"
            android:label="@string/title_activity_menu_tab_list" />
        <activity
            android:name=".views.menu.MainActivity"
            android:label="@string/title_activity_main" />
    </application>

</manifest>